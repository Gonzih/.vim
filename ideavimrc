source ~/.vim/plugin/keybindings.vim

map j j

nmap <leader>cl :action CommentByLineComment<cr>
nmap <leader>== :action ReformatCode<cr>
nmap <c-\> :action Flutter.Toolbar.ReloadAction<cr>
nmap <c-|> :action Flutter.Toolbar.ReloadAction<cr>
nmap gt :action NextTab<cr>
nmap gT :action PreviousTab<cr>
nmap <leader>bb :action RecentFiles<cr>
nmap <leader>pf :action SearchEverywhere<cr>

nmap jj :action VimDeleteLine<cr>
vmap j :action VimDeleteVisual<cr>

set vim-surround

noremap js ds
